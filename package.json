    "react-draggable": "^3.0.4",
    "react-responsive": "^4.0.3"
  },
  "bugs": {
    "url": "https://github.com/samguyjones/mobinge/issues"
  },
  "homepage": "https://github.com/samguyjones/mobinge#readme",
