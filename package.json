{
  "name": "mobinge",
  "version": "0.0.2",
  "engines": "v6.12.0",
  "author": "Sam Jones",
  "description": "Client-heavy webcomic site display system",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samguyjones/mobinge.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "concat": "^1.0.3",
    "webpack": "^3.8.1"
  },
  "scripts": {
